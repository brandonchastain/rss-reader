{
  "routes": [
    {
      "route": "/api/healthz",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/_framework/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/.auth/*"]
  },
  "globalHeaders": {
    "cache-control": "no-cache"
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "platform": {
    "apiRuntime": "node:20"
  }
}